            <inertia ixx="4.93711727641837E-06" ixy="6.02977678312998E-13" ixz="-2.32505912287766E-12" iyy="4.40383720688213E-06" iyz="5.04733188504208E-07" izz="5.43920075735643E-07" />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
