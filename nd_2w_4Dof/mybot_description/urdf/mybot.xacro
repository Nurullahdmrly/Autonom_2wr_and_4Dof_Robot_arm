        </collision>
    </link>
    <joint name="joint_1" type="revolute">
        <origin xyz="0 0 0.06" rpy="0 0 0" />
        <parent link="base_link" />
        <child link="link_1" />
        <axis xyz="0 0 1" />
        <limit lower="-3.14" upper="3.14" effort="300" velocity="1" />
    </joint>
    <link name="link_2">
        
<inertial>
